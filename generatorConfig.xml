<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <context id="DB2Tables" targetRuntime="MyBatis3">
        <!--配置数据库连接-->

        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://10.16.16.14:3306/azeroth?zeroDateTimeBehavior=convertToNull&amp;useSSL=true"
                        userId="dev_crm"
                        password="ziroomdb">
        </jdbcConnection>

        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>


        <!--指定javaBean生成的位置-->
        <javaModelGenerator
                targetPackage="com.ziroom.azeroth.entity.AchievementUser"

                targetProject="src">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
       <!-- 指定sql映射文件生成的位置-->
        <sqlMapGenerator
                targetPackage="com.ziroom.azeroth.mapper.azeroth"
                targetProject="src">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>
         <!--指定dao接口生成位置-->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.ziroom.azeroth.mapper.azeroth"
                             targetProject="src">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>
        <!-- 指定每个表的生成策略-->
        <table  tableName="t_arena_realtime" domainObjectName="ArenaRealTime" ></table>
    </context>
</generatorConfiguration>